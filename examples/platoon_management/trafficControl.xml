<?xml version="1.0" encoding="UTF-8"?>

<trafficControl id="example_0">

    <speed id="veh" begin="10" value="20" />  
    
    <optSize begin="40" value="5" />
    <optSize begin="60" value="3" />
    
    <pltMerge pltId="veh.3" begin="80" />
    
    <pltSplit pltId="veh" splitIndex="2" begin="100" />
    <pltSplit splitVehId="veh.5" begin="120" />
    
    <optSize begin="140" value="6" />
    
    <pltLeave pltId="veh" leaveIndex="5" begin="160" leaveDirection="left"/>
    <pltLeave pltId="veh" leaveIndex="3" begin="180"/>
    <pltLeave pltId="veh" leaveIndex="0" begin="210"/>
    
</trafficControl>


<trafficControl id="control_0">

    <speed id="veh.0" begin="60" end="1000" value="28*(-1)^floor(2(t-60)/200)" />  
    
</trafficControl>


<trafficControl id="control_1">

    <speed id="veh.0" begin="60" end="1000" value="28*(-1)^floor(2(t-60)/200)" maxAccel="1" maxDecel="7" />  
    
</trafficControl>


<trafficControl id="control_2">

    <speed id="veh.0" begin="60" end="1000" value="18+10*sin(0.1*(t-60))" />  
    
</trafficControl>
