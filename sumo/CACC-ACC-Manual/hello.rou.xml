<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/routes_file.xsd">

  <!-- Manual driving vehicle that uses KraussFixed CFModel -->
  <vType id="TypeManual" length="5.0" minGap="2.0" maxSpeed="30.0" >
      <carFollowing-KraussFixed MaxAccel="3.0" MaxDecel="5.0" T_g="1.64" sigma="0" tau="1" />
  </vType>

  <!-- ACC vehicle that uses ACC CFModel -->
  <vType id="TypeACC" length="5.0" minGap="2.0" maxSpeed="30.0" >
      <carFollowing-ACC MaxAccel="3.0" MaxDecel="5.0" T_g="1.54" tau="0.5" ComfAccel="2.0" ComfDecel="3.0" K_sc="0.4" K_v="1.0" K_g="5.0" V_int="30.0" />
  </vType>

  <!-- CACC vehicle (trajectory for testing stability) -->
  <vType id="TypeCACC0" length="5.0" minGap="2.0" maxSpeed="30.0" >
      <carFollowing-CACC MaxAccel="1.5" MaxDecel="2.0" T_g="0.71" tau="0.4" ComfAccel="2.0" ComfDecel="3.0" K_sc="0.4" K_v="0.99" K_g="4.08" K_a="0.66" V_int="30.0" />
  </vType>

  <!-- CACC vehicle (representing a normal vehicle) that uses CACC CFModel -->
  <vType id="TypeCACC1" length="5.0" minGap="2.0" maxSpeed="30.0" >
      <carFollowing-CACC MaxAccel="3.0" MaxDecel="5.0" T_g="0.55" tau="0.4" ComfAccel="2.0" ComfDecel="3.0" K_sc="0.4" K_v="0.99" K_g="4.08" K_a="0.66" V_int="30.0" />
  </vType>
  
  <!-- CACC vehicle (representing a truck, bus, etc) that uses CACC CFModel -->
  <vType id="TypeCACC2" length="5.0" minGap="2.0" maxSpeed="30.0" >
      <carFollowing-CACC MaxAccel="3.0" MaxDecel="3.0" T_g="0.71" tau="0.4" ComfAccel="2.0" ComfDecel="3.0" K_sc="0.4" K_v="0.99" K_g="4.08" K_a="0.66" V_int="30.0" />
  </vType>

  <route id="route1" edges="1to2 2to3 3to4 4to6" />

  <vehicle id="CACC1" type="TypeCACC1" route="route1" depart="1" />
  <vehicle id="CACC2" type="TypeCACC1" route="route1" depart="3" />
  <vehicle id="CACC3" type="TypeCACC1" route="route1" depart="5" />
  <vehicle id="CACC4" type="TypeCACC1" route="route1" depart="7" />
  <vehicle id="CACC5" type="TypeCACC1" route="route1" depart="9" />
  <vehicle id="CACC6" type="TypeCACC1" route="route1" depart="11" />
  <vehicle id="CACC7" type="TypeCACC1" route="route1" depart="13" />
  <vehicle id="CACC8" type="TypeCACC1" route="route1" depart="15" />
  <vehicle id="CACC9" type="TypeCACC1" route="route1" depart="17" />
  <vehicle id="CACC10" type="TypeCACC1" route="route1" depart="19" />

</routes>
